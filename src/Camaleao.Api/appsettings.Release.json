{
  "Info": {
    "Title": "Camaleão",
    "Version": "v2",
    "Description": "Camaleão Mock Service",
    "TermsOfService": "Terms of service",
    "Contact": {
      "Name": "Mundipagg",
      "Email": "tgambarra@mundipagg.com"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "MongoConnectionString": "#{Conf_MongoConnectionString}#",
  "MongoDatabase": "#{Conf_MongoDatabase}#",
  "HostUrl": "#{Conf_HostUrl}#",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "#{Conf_LogUrl}#",
          "apiKey": "#{Conf_LogApiKey}#"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Camaleao"
    }
  }
}
